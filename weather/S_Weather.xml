<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <serviceStateTable>
    <stateVariable>
      <name>Location</name>
      <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>string</dataType>
      <defaultValue>Enter an implementation-specific Location string (eg. ZIP Code)</defaultValue>
    </stateVariable>
    <stateVariable>
      <name>LocationUsed</name>
      <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>string</dataType>
      <defaultValue></defaultValue>
    </stateVariable>
    <stateVariable>
      <name>Metric</name>
      <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>boolean</dataType>
      <defaultValue>0</defaultValue>
    </stateVariable>
    <stateVariable>
      <name>Period</name>
      <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>ui4</dataType>
      <defaultValue>1800</defaultValue>
    </stateVariable>
    <stateVariable>
      <name>Condition</name>
      <sendEventsAttribute>yes</sendEventsAttribute>
      <dataType>string</dataType>
      <defaultValue></defaultValue>
      <shortCode>condition</shortCode>
    </stateVariable>
    <stateVariable>
      <name>ConditionGroup</name>
      <sendEventsAttribute>yes</sendEventsAttribute>
      <dataType>string</dataType>
      <defaultValue></defaultValue>
      <shortCode>conditiongroup</shortCode>
    </stateVariable>
    <stateVariable>
      <name>WindCondition</name>
      <sendEventsAttribute>yes</sendEventsAttribute>
      <dataType>string</dataType>
      <defaultValue></defaultValue>
      <shortCode>windcondition</shortCode>
    </stateVariable>
    <stateVariable>
      <name>WindDirection</name>
      <sendEventsAttribute>yes</sendEventsAttribute>
      <dataType>string</dataType>
      <defaultValue>NA</defaultValue>
      <shortCode>winddirection</shortCode>
    </stateVariable>
    <stateVariable>
      <name>WindSpeed</name>
      <sendEventsAttribute>yes</sendEventsAttribute>
      <dataType>i4</dataType>
      <defaultValue>-1</defaultValue>
      <shortCode>windspeed</shortCode>
    </stateVariable>
    <stateVariable>
      <name>ProviderKey</name>
      <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>string</dataType>
      <defaultValue></defaultValue>
      <shortCode>providerkey</shortCode>
    </stateVariable>    <stateVariable>
      <name>ProviderURL</name>
      <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>string</dataType>
      <defaultValue></defaultValue>
      <shortCode>providerurl</shortCode>
    </stateVariable>    <stateVariable>
      <name>ProviderName</name>
      <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>string</dataType>
      <defaultValue></defaultValue>
      <shortCode>providername</shortCode>
    </stateVariable>
  </serviceStateTable>
  <actionList>
    <action>
      <name>SetUnitsMetric</name>
    </action>
    <action>
      <name>SetUnitsUS</name>
    </action>
    <action>
      <name>SetLocation</name>
      <argumentList>
        <argument>
          <name>location</name>
          <direction>in</direction>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetPeriod</name>
      <argumentList>
        <argument>
          <name>period</name>
          <direction>in</direction>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetProviderKey</name>
      <argumentList>
        <argument>
          <name>key</name>
          <direction>in</direction>
        </argument>
      </argumentList>
    </action>
  </actionList>
</scpd>
